{
  "name": "meson",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "integer_literal"
    },
    "integer_literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "decimal_literal"
        },
        {
          "type": "SYMBOL",
          "name": "octal_literal"
        },
        {
          "type": "SYMBOL",
          "name": "hex_literal"
        }
      ]
    },
    "decimal_literal": {
      "type": "SYMBOL",
      "name": "_DECIMAL_NUMBER"
    },
    "_DECIMAL_NUMBER": {
      "type": "PATTERN",
      "value": "[1-9][0-9]*"
    },
    "octal_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "0o"
        },
        {
          "type": "SYMBOL",
          "name": "_OCTAL_NUMBER"
        }
      ]
    },
    "_OCTAL_NUMBER": {
      "type": "PATTERN",
      "value": "[0-7]+"
    },
    "hex_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "0x"
        },
        {
          "type": "SYMBOL",
          "name": "_HEX_NUMBER"
        }
      ]
    },
    "_HEX_NUMBER": {
      "type": "PATTERN",
      "value": "[a-fA-F0-9]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

